{
  "name": "avatarlabs/teal_modules",
  "description": "Laravel Module management",
  "require": {
    "php": ">=5.5",
    "laravel/framework": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7",
    "mockery/mockery": "~0.9",
    "orchestra/testbench": "^3.1|^3.2|^3.3|^3.4"
  },
  "autoload": {
    "psr-4": {
      "Avatarlabs\\Modules\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Avatarlabs\\Modules\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Avatarlabs\\Modules\\LaravelModulesServiceProvider"
      ],
      "aliases": {
        "Module": "Avatarlabs\\Modules\\Facades\\Module"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
